{
	"title": "books",
	"author": "Cleon",
	"description": "写写自己读书或者感想或者发现或者乱七八糟",
	"language": "zh-hans",
	"links": {
		"sidebar": {
			"百度": "https://www.baidu.com",
			"谷歌": "http://www.google.com"
		}
	},
	"plugins": [
		"back-to-top-button",
		"chapter-fold",
		"expandable-chapters-small",
		"-sharing",
		"sharing-plus",
		"insert-logo",
		"code",
		"todo",
		"-lunr",
		"-search",
		"search-pro",
		"advanced-emoji",
		"github",
		"emphasize",
		"splitter",
		"page-copyright",
		"sectionx",
		"klipse",
		"flexible-alerts",
		"click-reveal",
		"popup",
		"hide-element"
	],
	"pluginsConfig": {
		"github": {
			"url": "https://github.com/xiaocheng2014"
		},
		"sharing": {
			"qq": true,
			"qzone": true,
			"weibo": true,
			"all": ["qq", "qzone", "weibo"]
		},
		"page-copyright": {
			"description": "description",
			"signature": "人生而自由，却无往不在枷锁中",
			"wisdom": "wisdom",
			"format": "YYYY-MM-dd hh:mm:ss",
			"copyright": "Copyright &#169; Cleon 小城",
			"timeColor": "#666",
			"copyrightColor": "#666",
			"utcOffset": "8",
			"style": "normal",
			"noPowered": ""
		},
		"hide-element": {
			"elements": [".gitbook-link"]
		}
	}
}
