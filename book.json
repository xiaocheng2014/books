{
	"title": "books",
	"author": "Cleon",
	"description": "写写自己读书或者感想或者发现或者乱七八糟",
	"language": "zh-hans",
	"links": {
		"sidebar": {
			"百度": "https://www.baidu.com",
			"谷歌": "http://www.google.com"
		}
	},
	"plugins": [
		"back-to-top-button",
		"chapter-fold",
		"expandable-chapters-small",
		"-sharing",
		"sharing-plus",
		"code",
		"todo",
		"-lunr",
		"-search",
		"search-pro",
		"advanced-emoji",
		"github",
		"emphasize",
		"splitter",
		"tbfed-pagefooter",
		"sectionx",
		"klipse",
		"flexible-alerts",
		"click-reveal",
		"popup",
		"hide-element"
	],
	"pluginsConfig": {
		"github": {
			"url": "https://github.com/xiaocheng2014"
		},
		"sharing": {
			"qq": true,
			"qzone": true,
			"weibo": true,
			"all": ["qq", "qzone", "weibo"]
		},
		"tbfed-pagefooter": {
			"copyright": "Copyright &#169 Cleon",
			"modify_label": "modified at",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"hide-element": {
			"elements": [".gitbook-link"]
		}
	}
}
